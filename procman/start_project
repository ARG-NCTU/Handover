cmd "vx300s_setup" {
    exec = "roslaunch dualarm_bringup dualarm_control.launch";
    host = "localhost";
}
group "00_handover" {   
    cmd "handover_server" {
        exec = "roslaunch handover handover_server.launch";
        host = "localhost";
    }
    cmd "handover_client" {
        exec = "roslaunch handover handover_client.launch";
        host = "localhost";
    }
    
}

group "01_sensor" {   
    cmd "01_camera" {
        exec = "roslaunch dualarm_bringup dual_camera.launch";
        host = "localhost";
    }
    cmd "01_force_sensor" {
        exec = "roslaunch robotiq_ft_sensor ft_sensor.launch";
        host = "localhost";
    }
}

cmd "Rviz" {
    exec = "rviz -d rviz/rviz.rviz";
    host = "localhost";
}

script "start_project" {

}